@using Microsoft.AspNetCore.Http;
@model FizzBuzz.Pages.LiczbaModel
@{
    ViewData["Title"] = "Zapisane w sesji";
}
<p>Dane w zapisane w sesji:</p>
@if (Model.Liczba != null)
{
    <p>
        @Html.DisplayNameFor(model => Model.Liczba.liczba):
